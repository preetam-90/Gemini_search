#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/pk/Desktop/Gemini-Search/node_modules/.pnpm/drizzle-kit@0.27.2/node_modules/drizzle-kit/node_modules:/home/pk/Desktop/Gemini-Search/node_modules/.pnpm/drizzle-kit@0.27.2/node_modules:/home/pk/Desktop/Gemini-Search/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/pk/Desktop/Gemini-Search/node_modules/.pnpm/drizzle-kit@0.27.2/node_modules/drizzle-kit/node_modules:/home/pk/Desktop/Gemini-Search/node_modules/.pnpm/drizzle-kit@0.27.2/node_modules:/home/pk/Desktop/Gemini-Search/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/drizzle-kit@0.27.2/node_modules/drizzle-kit/bin.cjs" "$@"
else
  exec node  "$basedir/../.pnpm/drizzle-kit@0.27.2/node_modules/drizzle-kit/bin.cjs" "$@"
fi
